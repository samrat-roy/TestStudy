{
  "name": "gapi_customer_order_v2",
  "version": "1.0.0",
  "description": "A NodeJs Based Customer Order API.",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node build-scripts/start-message.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "open:src": "nodemon --delay 3 src/index.js --exec babel-node ",
    "security-check": "nsp check",
    "lint": "esw src build-scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress --compilers js:babel-core/register \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "docker": "npm-run-all --parallel start-inmemorydb server",
    "server": "babel-node src/index.js",
    "start-inmemorydb": "json-server src/resources/local/index.js --port 3002",
    "api": "npm-run-all --parallel start-inmemorydb server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.dev.global.tesco.org/UKGrocery/GAPICustomerOrder_V2.git"
  },
  "keywords": [
    "CustomerOrder"
  ],
  "author": "Kaustubh Khati",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.0",
    "bunyan-format": "^0.2.1",
    "bunyan-rotating-file-stream": "^1.5.2",
    "cluster-service": "^2.0.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "enum": "^2.4.0",
    "express": "^4.15.2",
    "http-errors": "^1.6.1",
    "lodash": "^4.17.4",
    "mssql": "^4.0.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "wcf.js": "0.0.6",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "json-server": "^0.10.3",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3"
  }
}
